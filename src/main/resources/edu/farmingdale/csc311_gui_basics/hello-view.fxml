<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.Double?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Polyline?>
<?import javafx.scene.shape.QuadCurve?>
<?import javafx.scene.shape.Sphere?>

<VBox alignment="CENTER" prefHeight="305.0" prefWidth="346.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23.0.1" fx:controller="edu.farmingdale.csc311_gui_basics.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Polyline fill="CHARTREUSE" strokeType="INSIDE">
     <points>
       <Double fx:value="-50.0" />
       <Double fx:value="-40.0" />
       <Double fx:value="50.0" />
       <Double fx:value="-40.0" />
       <Double fx:value="0.0" />
       <Double fx:value="60.0" />
       <Double fx:value="-50.0" />
       <Double fx:value="-40.0" />
     </points>
   </Polyline>
   <Sphere radius="50" />
   <QuadCurve controlX="-50.0" controlY="-100.0" endX="50.0" fill="DODGERBLUE" startX="-50.0" stroke="BLACK" strokeType="INSIDE" />

    <Label fx:id="welcomeText" />
    <Button onAction="#onHelloButtonClick" prefHeight="0.0" prefWidth="49.0" text="Hello!" />
</VBox>
